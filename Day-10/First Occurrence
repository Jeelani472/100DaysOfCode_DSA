public class firstoccurence {
    public static int fo(int arr[],int i,int key){
        if(i==arr.length){
            return -1;
        }
        if(arr[i]==key){
            return i;
        }
        return fo(arr,i+1,key);
    }
    public static void main(String[] args) {
        int arr[]={5,3,1,2,3,4,3};
        int i=0;
        int key=3;
        System.out.println(fo(arr,i,key));
    }
}
